<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/">
    <ShortName>ElliotForWater</ShortName>
    <Description>Elliot For Water, the Search Engine to donate water with each click</Description>
    <InputEncoding>UTF-8</InputEncoding>
    <LongName>Elliot For Water Search Engine to donate water with each click</LongName>
    <Image width="16" height="16" type="image/x-icon">https://elliotforwater.com/images/metas/favicon-16x16.png</Image>
    <Url type="text/html" template="https://elliotforwater.com/search">
      <Param name="query" value="{searchTerms}"/>
      <Param name="type" value="web"/>
    </Url>
    <Url type="application/x-suggestions+json" template="https://suggest.finditnowonline.com/SuggestionFeed/Suggestion?format=jsonp&amp;gd=SY1002042&amp;q={searchTerms}"/>
</OpenSearchDescription>

